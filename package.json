{
  "name": "jchl",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "build": "node build.js",
    "start": "electron .",
    "packageDarwin": "electron-packager .  --platform=darwin --arch=x64 --out=./dist --asar  --icon=icon.icns",
    "packageWin": "electron-packager . --platform=win32 --arch=x64  --out=./dist --asar --icon icon.ico --ignore=\"(dist|dest|docs|.gitignore|LICENSE|README.md)\"",
    "packageLinux": "electron-packager .  --platform=linux --arch=x64 --out=./dist --asar --icon icon.ico"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^3.0.3",
    "electron-packager": "^12.2.0",
    "electron-winstaller": "^2.7.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
